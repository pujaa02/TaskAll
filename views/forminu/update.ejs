          <p id="p10" class="error"></p>
        </div>
      </div>

      <div class="row">
        <div class="col">
          <label for="state" class="form-label">State :</label>
          <% let state=result[0].state %>
          <select class="custom-select" id="state" name="state">
            <%if(state=="Gujarat"){%>
            <option value="Gujarat" selected>Gujarat</option>
            <%}else{ %>
            <option value="Gujarat">Gujarat</option>
            <% } %> <%if(state=="Gua"){%>
            <option value="Gua" selected>Gua</option>
            <%}else{ %>
            <option value="Gua">Gua</option>
            <% } %> <%if(state=="Haryana"){%>
            <option value="Haryana" selected>Haryana</option>
            <%}else{ %>
            <option value="Haryana">Haryana</option>
            <% } %> <%if(state=="Bihar"){%>
            <option value="Bihar" selected>Bihar</option>
            <%}else{ %>
            <option value="Bihar">Bihar</option>
            <% } %>
          </select>
        </div>
        <div class="col">
          <label for="rel_status" class="form-label">Relationship Status :</label>
          <% let status=result[0].rel_status %>
          <select class="custom-select" id="relstatus" name="relstatus">
            <%if(status=="married"){%>
            <option value="married" name="relstatus" selected>
              Married
            </option>
            <% }else{ %>
            <option value="married" name="relstatus">Married</option>
            <% } %> <%if(status=="unmarried"){%>
            <option value="unmarried" name="relstatus" selected>
              Unmarried
            </option>
            <% }else{ %>
            <option value="unmarried" name="relstatus">Unmarried</option>
            <% } %> <%if(status=="divorced"){%>
            <option value="divorced" name="relstatus" selected>
              Divorced
            </option>
            <% }else{ %>
            <option value="divorced" name="relstatus">Divorced</option>
            <% } %> <%if(status=="widow"){%>
            <option value="widow" name="relstatus" selected>Widow</option>
            <% }else{ %>
            <option value="widow" name="relstatus">Widow</option>
            <% } %>
          </select>
        </div>
      </div>
      <br />
      <div class="row">
        <div class="col">
          <label for="Gender" class="form-label" id="gender" name="gender">Gender :</label>
          <div class="form-check form-check-inline">
            <% let x=result[0].gender %> <%if(x=="male"){%>
            <input class="form-check-input" type="radio" name="gender" id="inlineRadio1" value="male" checked="checked" />
            <label class="form-check-label" for="inlineRadio1">Male</label>
            <% }else{ %>
            <input class="form-check-input" type="radio" name="gender" id="inlineRadio1" value="male" />
            <label class="form-check-label" for="inlineRadio1">Male</label>
            <% } %>
          </div>
          <% let y=result[0].gender %> <%if(y=="female"){%>
          <div class="form-check form-check-inline">
            <input class="form-check-input" type="radio" name="gender" id="inlineRadio2" value="female" checked="checked" />
            <label class="form-check-label" for="inlineRadio2">Female</label>
            <% }else{ %>
            <input class="form-check-input" type="radio" name="gender" id="inlineRadio2" value="female" />
            <label class="form-check-label" for="inlineRadio2">Female</label>
            <% } %>
          </div>
          <div class="form-check form-check-inline">
            <% let z=result[0].gender %> <%if(z=="other"){%>
