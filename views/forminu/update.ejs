  <fieldset class="form-control" style="width: 650px">
    <legend><b>Basic Details</b></legend>

    <div class="container">
      <div class="row">
        <div class="col">
          <label for="fname">First name:</label>
          <input type="text" class="form-control" id="fname" name="fname" placeholder="Enter your first Name...." tabindex="1" value="<%=(result[0].fname)%>" autofocus />
          <p id="p1" class="error"></p>
        </div>
        <div class="col">
          <label for="lname">Last name:</label>
          <input type="text" class="form-control" id="lname" name="lname" value="<%=(result[0].lname)%>" placeholder="Enter your Last Name...." tabindex="2" />
          <p id="p2" class="error"></p>
        </div>
      </div>

      <div class="row">
        <div class="col">
          <label for="designation">Designation:</label>
          <input type="text" class="form-control" id="designation" name="designa" value="<%=(result[0].designation)%>" tabindex="3" />
          <p id="p3" class="error"></p>
        </div>
        <div class="col">
          <label for="dob" class="form-label">Date of Birth :</label>
          <input type="text" class="form-control" id="dob" name="dob" rows="1" tabindex="4" value="<%=(result[0].bd)%>" />
          <p id="p4" class="error"></p>
        </div>
      </div>

      <div class="row">
        <div class="col">
          <label for="exampleFormControlInput1" class="form-label">Email address</label>
          <input type="text" class="form-control" id="email" name="email" placeholder="name@abc.com" autocomplete="email" tabindex="5" value="<%=(result[0].email)%>" />
          <p id="p5" class="error"></p>
        </div>
        <div class="col">
          <label for="phone_no">Phone Number:</label>
          <input type="text" class="form-control" id="number" name="number" tabindex="6" value="<%=(result[0].phone)%>" />
          <p id="p6" class="error"></p>
        </div>
      </div>

      <div class="row">
        <div class="col">
          <label for="city" class="form-label">City :</label>
          <input class="form-control" id="city" name="city" list="cities" rows="1" placeholder="Enter city Here" class="col-sm-6 custom-select custom-select-sm" tabindex="7" value="<%=(result[0].city)%>" />
          <p id="p7" class="error"></p>
          <datalist id="cities">
            <option value="surat">surat</option>
            <option value="ahmedabad">ahmedabad</option>
            <option value="vadodara">vadodara</option>
            <option value="mehasana">mehasana</option>
            <option value="rajkot">rajkot</option>
            <option value="junagadh">junagadh</option>
            <option value="somnath">somnath</option>
          </datalist>
        </div>
        <div class="col">
          <label for="zip_code" class="form-label">Zip code :</label>
          <input type="text" class="form-control" id="zipcode" name="zipcode" rows="1" tabindex="8" value="<%=(result[0].zipcode)%>" />
          <p id="p8" class="error"></p>
        </div>
      </div>

      <div class="row">
        <div class="col">
          <label for="exampleFormControlTextarea1" class="form-label">Address 1 :</label>
          <input class="form-control" id="add1" name="add1" rows="1" tabindex="9" value="<%=(result[0].address1)%>" />
          <p id="p9" class="error"></p>
        </div>
      </div>
      <div class="row">
        <div class="col">
